  pop();

  memoriesFlow();
}

function lastStanza() {
  tint(255, 255);

  lastVideo();

  bathtubVideo();
  bathtub.volume(0.3);

  sinkVideo();
  sink.volume(0.3);

  gradualVideo();
  gradual.volume(0.3);

  tint(255, 70);
  //underlying

  snickeringVideo();
  snickering.volume(0.0);

  hairVideo();
  hair.volume(0.0);

  showerVideo();
  shower.volume(0.0);

  blackheadVideo();
  blackhead.volume(0.0);

  webcamCapture();

  standardFont();

  text("I remember white rubber sink plugs on chains.", 27, 430, 250, 300);

  text("I remember not to stand up in the bathtub because I might slip and fall and bash my head open. ", 332, 400, 250, 300);

  text("I remember ‘This is the last time I’m going to tell you.’", 25, 100, 250, 250);

  push();
  textSize(15);
  text("I remember when the word ‘fairy’ began to evoke snickering not knowing why. Then later, I do remember knowing why. What I don’t remember is how I learned what it meant. Just a gradual process of putting two and two together, I guess. Plus a bit of speculation.", 332, 75, 250, 250);
  pop();

  memoriesFlow();
}

