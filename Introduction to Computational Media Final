  isolationVideo();
  isolation.volume(0.3);

  hospitalVideo();

  partyVideo();
  party.volume(0.3);

  standardFont();

  tint(255, 70);

  dejavuVideo();
  dejavu.volume(0.0);

  webcamCapture();

  text("I remember those times of not knowing if you feel really happy or really sad. (Wet eyes and a high heart.)", 332, 80, 250, 250);

  text("I remember, in crowds - total isolation!", 27, 440, 250, 300);

  text("I remember, at parties - naked!", 332, 440, 250, 300);

  text("I remember body realizations about how fragile we (life) really are (is).", 25, 95, 250, 250);

  memoriesFlow();
}

