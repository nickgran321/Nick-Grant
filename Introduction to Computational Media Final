function snickeringStanza() {
  tint(255, 255);
  //current

  image(snickering.loop(), 0, height - 600, snickering.width / 3.2, snickering.height / 1.8);

  hairVideo();
  hair.volume(0.3);

  showerVideo();
  shower.volume(0.3);

  blackheadVideo();
  blackhead.volume(0.3);

  tint(255, 70);
  //underlying

  bedVideo();
  bed.volume(0.0);

  tosleepVideo();
  tosleep.volume(0.0);

  paintVideo();
  paint.volume(0.0);

  thinkingVideo();
  thinking.volume(0.0);

  webcamCapture();
  standardFont();

  text("I remember (hurt like hell) Saturday night hair washings of fingernails to scalp.", 27, 400, 250, 300);

  push();
  textSize(16);
  text("I remember predicting that probably someday in the future people would dye their all different colors from day to day to match whatever they happened to be wearing that day.", 332, 390, 250, 300);
  pop();

  text("I remember a teacher who used to use the word ‘queer’ a lot (meaning ‘unusual’) and a lot of snickering.", 25, 80, 250, 250);

  text("I remember my mother cornering me into corners to squeeze out blackheads. (Hurt like hell).", 332, 80, 250, 250);

  memoriesFlow();
}

function gradualStanza() {
  tint(255, 255);
  //current

  gradualVideo();

  snickeringVideo();
  snickering.volume(0.3);

  hairVideo();
  hair.volume(0.3);

  showerVideo();
  shower.volume(0.3);

  tint(255, 70);
  //underlying

  blackheadVideo();
  blackhead.volume(0.0);

  bedVideo();
  bed.volume(0.0);

  tosleepVideo();
  tosleep.volume(0.0);

  paintVideo();
  paint.volume(0.0);

  webcamCapture();

  standardFont();

  text("I remember (hurt like hell) Saturday night hair washings of fingernails to scalp.", 27, 400, 250, 300);

  push();
  textSize(16);
  text("I remember predicting that probably someday in the future people would dye their all different colors from day to day to match whatever they happened to be wearing that day.", 332, 390, 250, 300);
  pop();

  text("I remember a teacher who used to use the word ‘queer’ a lot (meaning ‘unusual’) and a lot of snickering.", 25, 80, 250, 250);

  push();
  textSize(15);
  text("I remember when the word ‘fairy’ began to evoke snickering not knowing why. Then later, I do remember knowing why. What I don’t remember is how I learned what it meant. Just a gradual process of putting two and two together, I guess. Plus a bit of speculation.", 332, 75, 250, 250);
  pop();

  memoriesFlow();
}

