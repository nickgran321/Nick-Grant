var state = 0;
let capture;
let memories = [];
let allLines;
let everything;
let words;
let r;
let g;
let b;

function preload() {
  dejavu = createVideo("dejavu.mp4");
  dejavu.hide();

  crying = createVideo("crying.mp4");
  crying.hide();

  isolation = createVideo("isolation.mp4");
  isolation.hide();

  party = createVideo("party.mp4");
  party.hide();

  hospital = createVideo("hospital.mp4");
  hospital.hide();

  thinking = createVideo("thinking.mp4");
  thinking.hide();

  paint = createVideo("paint.mp4");
  paint.hide();

  tosleep = createVideo("tosleep.mp4");
  tosleep.hide();

  bed = createVideo("bed.mp4");
  bed.hide();

  blackhead = createVideo("blackhead.mp4");
  blackhead.hide();

  shower = createVideo("shower.mp4");
  shower.hide();

  hair = createVideo("hair.mp4");
  hair.hide();

  snickering = createVideo("snickering.mp4");
  snickering.hide();

  gradual = createVideo("gradual.mp4");
  gradual.hide();

  sink = createVideo("sink.mp4");
  sink.hide();

  bathtub = createVideo("bathtub.mp4");
  bathtub.hide();

  last = createVideo("last.mp4");
  last.hide();

  why = createVideo("why.mp4");
  why.hide();

  birthday = createVideo("birthday.mp4");
  birthday.hide();

  icecream = createVideo("icecream.mp4");
  icecream.hide();

  umbrella = createVideo("umbrella.mp4");
  umbrella.hide();

  candles = createVideo("candles.mp4");
  candles.hide();

  roundd = createVideo("round.mp4");
  roundd.hide();

  donkey = createVideo("donkey.mp4");
  donkey.hide();

  corn = createVideo("corn.mp4");
  corn.hide();

  wheat = createVideo("wheat.mp4");
  wheat.hide();

  beef = createVideo("beef.mp4");
  beef.hide();

  beet = createVideo("beet.mp4");
  beet.hide();

  calendar = createVideo("calendar.mp4")
  calendar.hide();

  microscopic = createVideo("microscopic.mp4");
  microscopic.hide();

  claustrophobic = createVideo("claustrophobic.mp4");
  claustrophobic.hide();

  breathing = createVideo("breathing.mp4");
  breathing.hide();

  capture = createCapture(VIDEO);
  capture.size(500, 500);
  capture.hide();

  whitenoise = loadSound("whitenoise.mp3");
  allLines = loadStrings('I_Remember.txt');
}

function setup() {
  createCanvas(600, 600);
  for (let i = 0; i < 1; i++) {
  memories.push(new strand(random(width), random(height)));
  }
  everything = allLines.join(" ");
  words = everything.split(" ");
  frameRate(0.9);
}

function keyPressed() {
  if (keyCode === RIGHT_ARROW) {
    state++;
  }
  if (keyCode === RIGHT_ARROW) {
    memories.push(new strand(random(width), random(height)));
  }
}

function draw() {
  if (state == 0) {
    beginningState();
  } else if (state == 1) {
    toprightChange();
  } else if (state == 2) {
    bottomleftChange();
  } else if (state == 3) {
    webcam();
  } else if (state == 4) {
    dejavuStanza();
  } else if (state == 5) {
    cryingStanza();
  } else if (state == 6) {
    isolationStanza();
  } else if (state == 7) {
    partyStanza();
  } else if (state == 8) {
    hospitalStanza();
  } else if (state == 9) {
    thinkingStanza();
  } else if (state == 10) {
    paintStanza();
  } else if (state == 11) {
    tosleepStanza();
  } else if (state == 12) {
    bedStanza();
  } else if (state == 13) {
    blackheadStanza();
  } else if (state == 14) {
    showerStanza();
  } else if (state == 15) {
    hairStanza();
  } else if (state == 16) {
    snickeringStanza();
  } else if (state == 17) {
    gradualStanza();
  } else if (state == 18) {
    sinkStanza();
  } else if (state == 19) {
    bathtubStanza();
  } else if (state == 20) {
    lastStanza();
  } else if (state == 21) {
    whyStanza();
  } else if (state == 22) {
    birthdayStanza();
  } else if (state == 23) {
    icecreamStanza();
  } else if (state == 24) {
    umbrellaStanza();
  } else if (state == 25) {
    candlesStanza();
  } else if (state == 26) {
    roundStanza();
  } else if (state == 27) {
    donkeyStanza();
  } else if (state == 28) {
    cornStanza();
  } else if (state == 29) {
    wheatStanza();
  } else if (state == 30) {
    beefStanza();
  } else if (state == 31) {
    beetStanza();
  } else if (state == 32) {
    calendarStanza();
  } else if (state == 33) {
    microscopicStanza();
  } else if (state == 34) {
    claustrophobicStanza();
  } else if (state == 35) {
    breathingStanza();
  } else if (state == 36) {
    toprightgoneStanza();
  } else if (state == 37) {
    bottomleftgoneStanza();
  } else if (state == 38) {
    topleftgoneStanza();
  } else if (state == 39) {
    bottomrightgoneStanza();
  } else if (state == 40) {
    nothingStanza();
  }
  whitenoise.play();
}

function beginningState() {
  beginningSquares();
  whitenoise.setVolume(0.0);
  movingLines();

  stroke(0);
  strokeWeight(2)
  textFont("American Typewriter");
  textSize(30);
  fill(255);
  rememberText();

  text("visual memoir (pages 139-143)", 50, 480, 60);
  //third square

  textSize(14);
  text("press right to continue", 410, 565);
  //fourth square
}

function toprightChange() {
  beginningSquares()
  standardFont();
  whitenoise.setVolume(0.0);

  rememberText();

  text("visual memoir (pages 139-143)", 50, 480, 60);

  textSize(14);
  text("press right to continue", 410, 565);

  textAlign(CENTER);
  textSize(16);
  text("Joe Brainard was an American artist and writer. He was born on March 11, 1942, in Salem Arkansas, and spent his childhood in Tulsa, Oklahoma. At age 19, he moved to New York City where he joined a community of talented poets and painters.", 328, 68, 250, 300);
  //second square change
}

function bottomleftChange() {
  beginningSquares()
  standardFont();
  whitenoise.setVolume(0.0);

  rememberText();

  wellKnown();

  textAlign(LEFT);
  textSize(14);
  text("press right to continue", 410, 565);

  textAlign(CENTER);
  textSize(16);
  text("Joe Brainard was an American artist and writer. He was born on March 11, 1942, in Salem Arkansas, and spent his childhood in Tulsa, Oklahoma. At age 19, he moved to New York City where he joined a community of talented poets and painters.", 328, 68, 250, 300);
  //second square change

  memoriesFlow();
}

function webcam() {
  beginningSquares();
  standardFont();
  whitenoise.setVolume(0.2);
  webcamCapture();

  rememberText();

  wellKnown();

  textAlign(LEFT);
  textSize(14);
  text("press right to continue", 410, 565);

  textAlign(CENTER);
  textSize(16);
  text("Joe Brainard was an American artist and writer. He was born on March 11, 1942, in Salem Arkansas, and spent his childhood in Tulsa, Oklahoma. At age 19, he moved to New York City where he joined a community of talented poets and painters.", 328, 68, 250, 300);
  //second square change
  memoriesFlow();
}

function dejavuStanza() {
  beginningSquares();

  push();
  tint(255, 255);
  dejavuVideo();
  pop();

  standardFont();
  webcamCapture();

  wellKnown();

  textAlign(LEFT);
  textSize(14);
  text("press right to continue", 410, 565);

  textAlign(CENTER);
  textSize(16);
  text("Joe Brainard was an American artist and writer. He was born on March 11, 1942, in Salem Arkansas, and spent his childhood in Tulsa, Oklahoma. At age 19, he moved to New York City where he joined a community of talented poets and painters.", 328, 68, 250, 300);

  textAlign(CENTER);
  textSize(20);
  text("I remember finding myself in situations I all of a sudden feel (remember) I’ve been in before: a ‘repeat’ life flash.", 25, 80, 250, 250);

  memoriesFlow();
}

function cryingStanza() {
  beginningSquares();
  memoriesFlow();

  tint(255, 255);
  cryingVideo();
  dejavuVideo();
  dejavu.volume(0.3);

  webcamCapture();

  standardFont();

  wellKnown();

  textAlign(LEFT);
  textSize(14);
  text("press right to continue", 410, 565);

  textAlign(CENTER);
  textSize(20);
  text("I remember finding myself in situations I all of a sudden feel (remember) I’ve been in before: a ‘repeat’ life flash.", 25, 80, 250, 250);

  push();
  textSize(20);
  text("I remember those times of not knowing if you feel really happy or really sad. (Wet eyes and a high heart.)", 332, 80, 250, 250);
  pop();
}

function isolationStanza() {
  beginningSquares()
  memoriesFlow();

  tint(255, 255);

  dejavuVideo();
  dejavu.volume(0.3);
  textAlign(CENTER);

  cryingVideo();
  crying.volume(0.3);

  isolationVideo();

  webcamCapture();

  standardFont();

  textAlign(LEFT);
  textSize(14);
  text("press right to continue", 410, 565);


  textAlign(CENTER);
  textSize(20);
  text("I remember, in crowds - total isolation!", 27, 440, 250, 300);

  textAlign(CENTER);
  textSize(20);
  text("I remember those times of not knowing if you feel really happy or really sad. (Wet eyes and a high heart.)", 332, 80, 250, 250);

  textSize(20);
  text("I remember finding myself in situations I all of a sudden feel (remember) I’ve been in before: a ‘repeat’ life flash.", 25, 80, 250, 250);

  memoriesFlow();
}

function partyStanza() {
  tint(255, 255);
  dejavuVideo();
  dejavu.volume(0.3);

  cryingVideo();
  crying.volume(0.3);

  isolationVideo();
  isolation.volume(0.3);

  partyVideo();

  webcamCapture();

  standardFont();
  text("I remember finding myself in situations I all of a sudden feel (remember) I’ve been in before: a ‘repeat’ life flash.", 25, 80, 250, 250);

  text("I remember those times of not knowing if you feel really happy or really sad. (Wet eyes and a high heart.)", 332, 80, 250, 250);

  text("I remember, in crowds - total isolation!", 27, 440, 250, 300);

  text("I remember, at parties - naked!", 332, 440, 250, 300);

  memoriesFlow();
}

