function movingLines() {

  r = random(251);
  g = random(91);
  b = random(85, 105);

  for (var i = 0; i < words.length; i++) {
    fill(r, g, b);
    text(words[i], random(width), random(height))
  }
}

function standardFont() {
  stroke(0);
  strokeWeight(2)
  textFont("American Typewriter");
  fill(255);
  textAlign(CENTER);
  textSize(20);
}

function beginningSquares() {
  noStroke();
  fill(0, 63, 92);
  rect(0, 0, 300, 300);
  fill(71, 43, 92);
  rect(300, 0, 300, 300);
  fill(188, 72, 115);
  rect(0, 300, 300, 300);
  fill(251, 91, 90);
  rect(300, 300, 300, 300);
  //squares
}

function webcamCapture() {
  push();
  tint(255, 50);
  image(capture, 0, 0, 600, 600);
  pop();
}

class strand {

  constructor(x, y) {
    this.x = x;
    this.y = y;
    this.history = [];
  }

  update() {
    this.x = this.x + random(-5, 5);
    this.y = this.y + random(-5, 5);

    let c = createVector(this.x, this.y);

    this.history.push(c);
  }

  show() {
    stroke(251, 91, 90, 3);
    beginShape();
    for (let i = 0; i < this.history.length; i++) {
      let pos = this.history[i];   
      fill(71, 43, 92, 2);
      vertex(pos.x, pos.y); 
      endShape();
    }
  }
}

function memoriesFlow() {
  for (let i = 0; i < memories.length; i++) {
    memories[i].show();
    memories[i].update();
  }
}

